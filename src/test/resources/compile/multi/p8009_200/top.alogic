// @verilator-lint-off
network top {
    const uint N = 2;
    gen for (uint n < N) {
        in sync u16 a#[n];
        in sync u16 b#[n];
        out sync u16 c#[n];
    }

    gen for (uint n < N) {
        instance#[n] = new test_add();
    }
    gen for (uint n < N) {
        a#[n] -> instance#[n].a;
        b#[n] -> instance#[n].b;
        instance#[n].c -> c#[n];
    }

}
