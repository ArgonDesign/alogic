network top {
  in bool i_0;
  in bool i_1;
  out bool o_0;
  out bool o_1;

  inst_0 = new sub(INVERT=false);
  inst_1 = new sub(INVERT=true);

  i_0 -> inst_0.i; inst_0.o -> o_0;
  i_1 -> inst_1.i; inst_1.o -> o_1;
}
