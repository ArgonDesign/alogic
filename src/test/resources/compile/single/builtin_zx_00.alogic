fsm builtin_zx_00 {
  // Need to go through a symbol, as they are only evaulatable through the
  // Frontend during elaboration
  const uint C = 2;

  out wire u3 o;

  void main() {
    o = {1'b1, @zx(C, 1'b1)};
    fence;
  }
}

