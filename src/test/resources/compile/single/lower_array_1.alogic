fsm lower_array_1 {
  in       u2    waddr;
  in       bool  wdata;
  in       u2    raddr;
  out wire bool  rdata;

  bool store[4];

  void main() {
    store.write(waddr, wdata);
    rdata = store[raddr];
    fence;
  }
}
