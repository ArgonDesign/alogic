fsm gen_for_named_03 {
  in        u4 i;
  out wire  u4 o_0;
  out wire  u4 o_1;
  out wire  u4 o_2;

  void main() {
    gen for (uint n = 0 ; n < 3 ; n++) : block#[n] {
      u4 b#[n+10] = i + n;
    }
    o_0 = block#[0].b#[10];
    o_1 = block#[1].b#[11];
    o_2 = block#[2].b#[12];
    fence;
  }
}

