network pipeline_02 {
  in  sync ready u2 i;
  out sync ready u2 o;

  pipeline bool p;
  pipeline bool q;

  new fsm a {
    out sync ready pipeline;
    void main() {
      {p, q} = i.read();
      out.write();
      fence;
    }
  }

  a -> b;

  new fsm b {
    in sync ready pipeline;
    void main() {
      in.read();
      o.write({q, p});
      fence;
    }
  }
}
