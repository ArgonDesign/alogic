fsm control_for_01 {
  in  bool i;
  out bool o = 0;
  in  bool a;
  in  bool b;

  void main() {
    for (;i;) {
      o = a;
      for (;i;) {
        o = ~a;
      }
    }
    for (;i;) {
      o = b;
      for (;i;) {
        o = ~b;
      }
    }
  }
}
