network dictident_instance_3 {
  in  bool i_a;
  out bool o_a;
  in  bool i_b;
  out bool o_b;

  network inner {
    in  bool ii;
    out bool oo;
    ii -> oo;
  }

  gen for (uint N < 2) {
    inst#[N] = new inner();
  }

  i_a -> inst#[0].ii;
  inst#[0].oo -> o_a;
  i_b -> inst#[1].ii;
  inst#[1].oo -> o_b;
}
