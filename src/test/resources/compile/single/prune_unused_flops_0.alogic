

fsm prune_unused_flops_0 {

    in       u8 i;
    out wire u8 o;

    void main() {
        if (i[0]) {
            o.write(i);
        } else {
            u8 temp = i + 1;
            o.write(temp);
        }
        fence;
    }
}

