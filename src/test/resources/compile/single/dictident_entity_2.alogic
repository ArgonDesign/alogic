network dictident_entity_2 {
  in  u1 i_a;
  out u1 o_a;
  in  u2 i_b;
  out u2 o_b;

  gen for (uint N < 1) {
    network inner#[0] {
      in  u1 ii;
      out u1 oo;
      ii -> oo;
    }
    network inner#[1] {
      in  u2 ii;
      out u2 oo;
      ii -> oo;
    }
  }

  inner_a = new inner#[0];
  inner_b = new inner#[1];

  i_a -> inner_a.ii; inner_a.oo -> o_a;
  i_b -> inner_b.ii; inner_b.oo -> o_b;
}
