fsm inline_type_punned_struct_value {
  in i3[2] p_in;
  out wire bool p_out;

  struct s1_t {
    i3  data;
  }

  struct combin {
    static bool func() {
      // We will try to simplity this prior to structure splitting to a single
      // return statement by folding 'data' into 'p_in[0].data', but 'p_in[0]'
      // on it's own is not a struct type, so we need to check for that.
      s1_t s1 = p_in[0];
      i3 data = s1.data;
      return (data < 0s) ? 1'b0 : 1'b1;
    }
  }

  void main() {
    p_out = combin.func();
    fence;
  }
}
