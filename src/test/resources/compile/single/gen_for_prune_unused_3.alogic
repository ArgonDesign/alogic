network gen_for_prune_unused_3 {
  in bool p_i;

  out bool p_o4;
  out bool p_o9;

  fsm inner {
    param uint P;
    param bool Q;

    struct foo {
      uint(P) f;
    }

    const foo PP = P;
    const uint PPP = 'PP;

    out bool p_o;

    void main() {
      bool b = p_i;
      gen for (uint N = 0; N < PPP; N++) {
        b = ~b;
      }
      p_o.write(b ^ Q);
      fence;
    }
  }

  i_4 = new inner(P=4, Q=true);
  i_9 = new inner(P=9, Q=false);

  i_4.p_o -> p_o4;
  i_9.p_o -> p_o9;
}
