fsm control_for_11 {
  in  bool i;
  in  bool j;
  out bool o = 0;
  in  bool a;
  in  bool b;

  void main() {
    for (;;) {
      o = a;
      if (i) break; else if (j) continue;
      for (;;) {
        o = ~a;
        if (i) break; else if (j) continue;
        o = 0;
      }
    }
    for (;;) {
      o = b;
      if (i) break; else if (j) continue;
      for (;;) {
        o = ~b;
        if (i) break; else if (j) continue;
        o = 1;
      }
    }
  }
}
