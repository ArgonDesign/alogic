u2 incinvdub(u2 x) {
  u2 invdub(u2 x) {
    u2 dub(u2 x) {
      return x << 1;
    }
    return dub(~x);
  }
  return invdub(x + 1);
}

fsm nested_in_comb_func_01 {
  in u2 i;
  out wire u2 o;

  void main() {
    o = incinvdub(i);
    fence;
  }
}
