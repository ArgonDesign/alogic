// @ignore
network snoop_06 {
  in  sync ready bool i;
  out sync ready bool o;

  out wire bool move;

  new fsm inner {
    snoop sync ready bool s;
    void main() {
      move = s.move;
      fence;
    }
  }

  i -> o;
  o -> inner.s;
}
