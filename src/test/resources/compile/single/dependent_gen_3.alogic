network dependent_gen_3 {
  in  bool i_0;
  in  bool i_1;
  out bool o_0;
  out bool o_1;

  const bool P = false;
  const bool S = true;

  gen if (P) {
    const bool Q = false;
    const bool R = false;
  } else {
    const bool Q = true;
  }

  gen if (Q) {
    gen if (S) {
      const bool R = true;
    } else {
      const bool R = false;
    }
  }

  gen if (R) {
    i_0 -> o_0;
    i_1 -> o_1;
  } else {
    i_1 -> o_0;
    i_0 -> o_1;
  }
}
