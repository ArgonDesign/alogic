

fsm prune_unused_flops_1 {

    in  u8 i;
    out u8 o;

    void main() {

        u8 temp_1 = i+1;
        o.write(temp_1);

        fence;

        u8 temp_2 = i+2;
        o.write(temp_2);

        fence;
    }
}

