network gen_for_downwards_3 {
  in bool p_i;

  out bool p_o4;
  out bool p_o9;

  fsm inner {
    param int P;

    out bool p_o;

    void main() {
      bool b = p_i;
      gen for (int n = 0; n > -P; n--) {
        b = ~b;
      }
      p_o.write(b);
      fence;
    }
  }

  i_4 = new inner(P=4);
  i_9 = new inner(P=9);

  i_4.p_o -> p_o4;
  i_9.p_o -> p_o9;
}
