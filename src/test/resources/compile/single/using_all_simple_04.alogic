struct S {
  static bool not(bool i) { return ~i; }
  static bool xor(bool i, bool j) { return i ^ j; }
}

fsm using_all_simple_04 {
  in       bool ia;
  in       bool ib;
  out wire bool o;

  gen if (true) {
    using S.*;

    void main() {
      o = xor(not(ia), ib);
      fence;
    }
  }
}

