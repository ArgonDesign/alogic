network unsized_param {
  in  u1  i;
  out u1  o_a;
  out u10 o_b;

  fsm expander {
    param uint W0 = 32;

    in  u1       p_i;
    out uint(W0) p_o;

    void main() {
      p_o.write({W0{p_i.read()}});
      fence;
    }
  }

  inst_a = new expander(W0=1);
  inst_b = new expander(W0=10);

  i -> inst_a.p_i, inst_b.p_i;
  inst_a.p_o -> o_a;
  inst_b.p_o -> o_b;
}

