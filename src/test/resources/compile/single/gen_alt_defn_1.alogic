network gen_alt_defn_1 {
  in u2 p_in_0;
  in u2 p_in_1;
  out u4 p_out_u;
  out i4 p_out_s;

  struct si_t {i2 f;}
  struct so_t {i4 f;}
  struct ui_t {u2 f;}
  struct uo_t {u4 f;}

  fsm inner {
    param bool SIGNED;

    gen if (SIGNED) {
      in  si_t p_i_0;
      in  si_t p_i_1;
      out so_t p_o;
    } else {
      in  ui_t p_i_0;
      in  ui_t p_i_1;
      out uo_t p_o;
    }

    void main() {
      p_o.write('p_i_0.f * 'p_i_1.f);
      fence;
    }
  }

  inner_u = new inner(SIGNED=false);
  inner_s = new inner(SIGNED=true);

  p_in_0 -> inner_u.p_i_0, inner_s.p_i_0;
  p_in_1 -> inner_u.p_i_1, inner_s.p_i_1;

  inner_u.p_o -> p_out_u;
  inner_s.p_o -> p_out_s;
}
