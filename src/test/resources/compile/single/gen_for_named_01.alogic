fsm gen_for_named_01 {
  in        u4 i;
  out wire  u4 o_0;
  out wire  u4 o_1;

  void main() {
    gen for (uint n = 0 ; n < 2 ; n++) : block {
      u4 b#[n] = i + n;
    }
    o_0 = b#[0];
    o_1 = b#[1];
    fence;
  }
}
