struct s {
  bool x;
  bool get() { return x; }
  void set(bool v) { x = v; }
}

network method_pipeline_00 {
  in bool i;
  out wire bool o;

  pipeline s t;

  new fsm stage_0 {
    void main() {
      t.set(i);
      write;
      fence;
    }
  }

  stage_0 -> stage_1;

  new fsm stage_1 {
    void main() {
      read;
      o = t.get();
      fence;
    }
  }
}
